{
  "name": "zappr",
  "version": "0.1.0-alpha.1",
  "description": "Approvals for Github pull requests.",
  "main": "dist/server/server.min.js",
  "scripts": {
    "test": "DEBUG='zappr:test' MORGAN_THRESH=999 NODE_ENV=test BABEL_ENV=test SQLITE_FILE=':memory:' mocha --recursive --compilers js:babel-register",
    "clean": "rm -rf dist",
    "build-client": "webpack --config webpack.client.js",
    "build-server": "webpack --config webpack.server.js",
    "build": "parallelshell 'npm run build-client -- -p' 'npm run build-server'",
    "run-client": "webpack-dev-server --history-api-fallback --inline --no-info --config webpack.client.js --content-base dist/client",
    "run-server": "nodemon dist/server/server.min.js --watch dist",
    "run-all": "parallelshell 'npm run run-client -- -d' 'npm run build-server -- -dw' 'npm run run-server'",
    "start": "node dist/server/server.min.js"
  },
  "engines": {
    "node": ">=5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zalando/zappr.git"
  },
  "author": {
    "name": "Maximilian Fellner",
    "email": "maximilian.fellner@zalando.de",
    "url": "https://tech.zalando.com"
  },
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/zalando/zappr/issues"
  },
  "homepage": "https://github.com/zalando/zappr#readme",
  "devDependencies": {
    "babel-core": "6.3.x",
    "babel-eslint": "5.0.0-beta6",
    "babel-loader": "6.2.x",
    "babel-plugin-webpack-loaders": "0.1.x",
    "babel-preset-es2015": "6.3.x",
    "babel-preset-node5": "10.1.x",
    "babel-preset-react": "6.3.x",
    "babel-preset-stage-1": "6.3.x",
    "babel-preset-stage-3": "6.3.x",
    "babel-register": "6.3.x",
    "chai": "3.4.x",
    "css-loader": "0.23.x",
    "eslint": "1.10.x",
    "extract-text-webpack-plugin": "0.9.x",
    "file-loader": "0.8.x",
    "json-loader": "0.5.x",
    "mocha": "2.3.x",
    "mountebank": "bbyars/mountebank#ca16804",
    "nodemon": "1.8.x",
    "parallelshell": "2.0.x",
    "passport-strategy": "1.0.x",
    "rimraf": "2.5.x",
    "style-loader": "0.13.x",
    "supertest": "1.1.x",
    "temp": "0.8.x",
    "url-loader": "0.5.x",
    "webpack": "1.12.x",
    "webpack-dev-server": "1.14.x"
  },
  "dependencies": {
    "bootstrap": "3.3.6",
    "bootstrap-social": "4.11.0",
    "bootstrap-toggle": "2.2.1",
    "debug": "2.2.0",
    "font-awesome": "4.5.0",
    "glob": "6.0.2",
    "history": "1.17.0",
    "isomorphic-fetch": "2.2.0",
    "js-yaml": "3.4.6",
    "koa": "2.0.0-alpha.3",
    "koa-bodyparser": "3.0.0",
    "koa-convert": "1.2.0",
    "koa-generic-session": "1.10.1",
    "koa-morgan": "1.0.1",
    "koa-passport": "2.0.1",
    "koa-router": "7.0.0",
    "koa-static": "1.5.2",
    "nconf": "0.8.2",
    "passport": "0.3.2",
    "passport-github2": "0.1.9",
    "react": "0.14.3",
    "react-bootstrap": "0.28.1",
    "react-dom": "0.14.3",
    "react-redux": "4.0.5",
    "react-router": "1.0.3",
    "redux": "3.0.5",
    "redux-router": "1.0.0-beta5",
    "redux-simple-router": "1.0.2",
    "redux-thunk": "1.0.2",
    "request": "2.67.0",
    "sequelize": "3.17.1"
  },
  "optionalDependencies": {
    "pg": "4.4.x",
    "sqlite3": "3.1.x"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "node5",
          "stage-1",
          "react"
        ],
        "plugins": [
          [
            "babel-plugin-webpack-loaders",
            {
              "config": "./webpack.server.test.js",
              "verbose": false
            }
          ]
        ]
      }
    }
  }
}
