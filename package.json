    "classnames": "^2.2.6",
    "debug": "^2.6.9",
    "dottie": "^1.1.1",
    "font-awesome": "^4.7.0",
    "fuzzysearch": "^1.0.3",
    "glob": "^7.1.3",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.12.0",
    "koa": "^2.5.3",
    "koa-bodyparser": "^3.2.0",
    "koa-compress": "^2.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-etag": "^3.0.0",
    "koa-generic-session": "^1.11.7",
    "koa-morgan": "^1.0.1",
    "koa-passport": "^2.2.2",
    "koa-router": "^7.4.0",
    "koa-static": "^3.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.8.3",
    "nconf": "^0.8.5",
    "node-tokens": "^0.0.9",
    "node-uuid": "^1.4.8",
    "normalizr": "^2.3.1",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "passport-strategy": "^1.0.0",
    "prom-client": "^3.5.0",
    "react": "^15.6.2",
    "react-bootstrap": "^0.30.10",
    "react-cookie-banner": "^0.0.14",
    "react-document-title": "^2.0.3",
    "react-dom": "^15.6.2",
    "react-redux": "^4.4.9",
    "react-router": "^2.8.1",
    "react-time": "^4.3.0",
    "redux": "^3.7.2",
    "redux-router": "^1.0.0",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "sequelize": "^3.33.0",
    "umzug": "^1.12.0"
  },
  "optionalDependencies": {
    "pg": "^4.5.6"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "es2015-node5",
          "stage-1",
          "react"
        ],
        "plugins": [
          [
            "babel-plugin-webpack-loaders",
            {
              "config": "./webpack.server.test.js",
              "verbose": false
            }
          ]
        ]
      }
    }
  }
}
